{
  "$schema": "https://opencode.ai/config.json",
  "keybinds": {
    "leader": "ctrl+;",
    "app_exit": "ctrl+d",
    "session_child_cycle": "ctrl+i",
    "session_child_cycle_reverse": "ctrl+o",
    "messages_half_page_up": "ctrl+k",
    "messages_half_page_down": "ctrl+j",
    "messages_last": "ctrl+h",
    "input_newline": "shift+enter",
    "theme_list": "<leader>t",
    "session_timeline": "ctrl+y",
    "input_clear": "ctrl+c",
    "editor_open": "<leader>e",
    "sidebar_toggle": "<leader>;",
    "status_view": "<leader>s",
    "session_export": "<leader>x",
    "session_new": "<leader>n",
    "session_list": "<leader>l",
    "session_compact": "<leader>c",
    "messages_copy": "<leader>y",
    "messages_undo": "<leader>u",
    "messages_redo": "<leader>r",
    "model_list": "<leader>m",
    "command_list": "ctrl+p",
    "agent_list": "<leader>a",
    "agent_cycle": "tab",
    "agent_cycle_reverse": "shift+tab",
    "input_forward_delete": "ctrl+d",
    "input_paste": "ctrl+v",
    "input_submit": "enter",
    "history_previous": "up",
    "history_next": "down",
    "session_interrupt": "escape",
    "messages_last_user": "ctrl+l"
  }
}
