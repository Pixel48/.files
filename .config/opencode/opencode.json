{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "opencode-antigravity-auth@1.3.1"
  ],
  "provider": {
    "google": {
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingLevel": "low"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "minimal": {
              "thinkingLevel": "minimal"
            },
            "low": {
              "thinkingLevel": "low"
            },
            "medium": {
              "thinkingLevel": "medium"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5 (Antigravity)",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "antigravity-claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        },
        "antigravity-claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        },
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash (Gemini CLI)",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "gemini-2.5-pro": {
          "name": "Gemini 2.5 Pro (Gemini CLI)",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "gemini-3-flash-preview": {
          "name": "Gemini 3 Flash Preview (Gemini CLI)",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "gemini-3-pro-preview": {
          "name": "Gemini 3 Pro Preview (Gemini CLI)",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        }
      }
    }
  },
  "keybinds": {
    "agent_cycle": "tab",
    "agent_cycle_reverse": "shift+tab",
    "agent_list": "alt+a",
    "app_exit": "ctrl+c,ctrl+d,<leader>q",
    "command_list": "ctrl+p",
    "editor_open": "<leader>e,alt+e",
    "history_next": "down",
    "history_previous": "up",
    "input_clear": "ctrl+c",
    "input_newline": "shift+enter",
    "input_paste": "ctrl+v",
    "input_submit": "enter",
    "leader": "ctrl+;",
    "messages_copy": "<leader>y,alt+y",
    "messages_half_page_down": "ctrl+j",
    "messages_half_page_up": "ctrl+k",
    "messages_last": "ctrl+h",
    "messages_last_user": "ctrl+l",
    "messages_redo": "<leader>r,alt+shift+z,alt+shift+u",
    "messages_undo": "<leader>u,alt+u,alt+z",
    "model_list": "ctrl+m",
    "session_child_cycle": "ctrl+o",
    "session_child_cycle_reverse": "ctrl+i",
    "session_compact": "ctrl+s",
    "session_export": "alt+s",
    "session_interrupt": "escape",
    "session_list": "alt+l",
    "session_new": "ctrl+n",
    "session_parent": "ctrl+u",
    "session_timeline": "ctrl+y",
    "sidebar_toggle": "ctrl+b",
    "status_view": "<leader>s,ctrl+alt+shift+s",
    "theme_list": "<leader>t,ctrl+alt+shift+t"
  },
  "mcp": {
    "memory": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "enabled": true
    }
  }
}
